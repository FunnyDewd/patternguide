  <%
  var menuHtml = ""
  function iterateMenu( obj ) {
    menuHtml += "<ul>\n";
    for ( var key in obj ) {
      if ( obj.hasOwnProperty( key ) ) {
        menuHtml += "<li>";
        menuHtml += "<a href='#'>" + key + "</a>";
        if ( Object.keys( obj[ key ] ).length ) {
          menuHtml +
          iterateMenu( obj[ key ] );
        }
        menuHtml += "</li>\n";
      }
    }
    menuHtml += "</ul>";
    return menuHtml;
  }
  %>

  <div class="pg-offcanvas">

    <button name="pg-trigger">PatternGuide</button>


    <nav role="navigation" class="pg-global pg-slide">
      <%- iterateMenu( helpers.nav.global ) %>
    </nav>
  </div>



